function mi_modulo_preprocess_field(&$variables, $hook){
	
	if ($variables['element']['#field_name'] == 'field_json_valores') {

		$v = $variables['element']['#items']->getValue()[0];
		$v = "<script>let myjson = {$v['value']}; alert(myjson.message-field);</script>";

		$variables['element']['#items']->setContext($v);
		$variables['element']['#items']->setValue($v);


		ksm($variables['element']['#items']->getValue()[0]);

	}
}